{
  "name": "source-slicer",
  "displayName": "Source Slicer",
  "description": "Slice through source code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.69.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:source-slicer.sliceBackwardW",
    "onCommand:source-slicer.sliceForwardW",
    "onCommand:source-slicer.sliceBackwardF",
    "onCommand:source-slicer.sliceForwardF",
    "onCommand:source-slicer.inline"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "source-slicer.sliceBackwardW",
        "title": "Slice Backward (New Window)"
      },
      {
        "command": "source-slicer.sliceForwardW",
        "title": "Slice Forward (New Window)"
      },
      {
        "command": "source-slicer.sliceBackwardF",
        "title": "Slice Backward (Fold)"
      },
      {
        "command": "source-slicer.sliceForwardF",
        "title": "Slice Forward (Fold)"
      },
      {
        "command": "source-slicer.inline",
        "title": "Inline Call"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.69.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4",
    "@vscode/test-electron": "^2.1.5"
  }
}
